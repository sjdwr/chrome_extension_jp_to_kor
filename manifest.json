{
    "manifest_version": 2,

    "name": "jp_kor_papago",
    "description": "Translate jap to kor",
    "version": "1.0",
    "permissions": [
        "tabs",
        "https://*/*",
        "http://*/*",
		"storage"
    ],
    "background": {
        "scripts": ["background.js"]
    },

    "browser_action": {
        "default_title": "popup practice",      
        "default_popup": "popup.html"        
      },

      "content_scripts": [{
        "matches": [
          "https://*/*",
          "http://*/*"
        ],
        "js": ["jquery-3.4.1.min.js", "content.js"],
        "css" :["mycss.css"]
    }],
    "commands": {
        "_execute_browser_action": {
        "suggested_key": {
        "windows": "Ctrl+B",
        "mac": "Command+Shift+Y",
        "chromeos": "Ctrl+Shift+U",
        "linux": "Ctrl+Shift+J"
      }
    }
  }

}